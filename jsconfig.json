{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/components/*": [
        "src/component/*"
      ],
      "@/pages/*": [
        "src/pages/*"
      ],
      "@/redux/*": [
        "src/redux/*"
      ],
      "@/validation/*": [
        "src/validation/*"
      ],
      "@/images/*": [
        "public/images/*"
      ],
      "@/lib/*": [
        "src/lib/*"
      ],
      "@/fonts/*": [
        "public/fonts/*"
      ]
    }
  }
}